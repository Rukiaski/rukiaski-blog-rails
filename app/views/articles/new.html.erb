<h3>Creat an article</h3>

<%= form_for @article do |element| %>
    <p>
        <%= element.label :title %></br>
        <%= element.text_field :title %>
    </p>
    <p>
        <%= element.label :description %>
        <%= element.text_area :description %>
    </p>
    <p>
        <%= element.submit %>
    </p>
<% end %>